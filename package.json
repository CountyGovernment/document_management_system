{
  "name": "shelf",
  "version": "1.0.0",
  "description": "A document management system",
  "main": "server.js",
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "NODE_ENV=test npm run seed && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec ./server/tests --timeout 20000",
    "script": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "seed": "npm run migrate && sequelize db:seed:all",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate"
  },
  "author": "Julie Wanja",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "enzyme": "^2.8.2",
    "express": "^4.15.3",
    "html-webpack-plugin": "^2.28.0",
    "morgan": "^1.8.2",
    "path": "^0.12.7",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "react-addons-test-utils": "^15.5.1",
    "sinon": "^2.3.2"
  }
}
