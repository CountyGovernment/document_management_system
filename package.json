{
  "name": "shelf",
  "version": "1.0.0",
  "description": "A document management system",
  "main": "server.js",
  "engines": {
    "node": "6.11.1"
  },
  "scripts": {
    "start": "node bin/www",
    "open:client": "node bin/www",
    "lint": "node_modules/.bin/esw webpack.config.* client server",
    "lint:watch": "npm run lint -- --watch",
    "start:D": "webpack-dev-server",
    "start:dev": "nodemon ./bin/www",
    "test:client": "NODE_ENV=test istanbul cover _mocha --require ./client/tests/TestSetup.js -R client/tests/**/*.js",
    "test:server": "NODE_ENV=test istanbul cover _mocha --report lcovonly -- -R spec ./server/tests --timeout 20000",
    "travis:test": "istanbul cover _mocha client/tests/**/*.js -- -R spec",
    "test": "npm run test:client && npm run test:server",
    "script": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "seed": "npm run migrate && sequelize db:seed:all",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "cover": "istanbul cover _mocha -R ./server/tests",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "author": "Julie Wanja",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jm-wanja/document_management_system.git"
  },
