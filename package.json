{
  "name": "shelf",
  "version": "1.0.0",
  "description": "A document management system",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel open:client lint:watch",
    "open:client": "babel-node ./appServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* client tools",
    "lint:watch": "npm run lint -- --watch",
    "start:D": "webpack-dev-server",
    "start:dev": "nodemon ./bin/www",
    "test": "NODE_ENV=test npm run seed && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec ./server/tests --timeout 20000",
    "script": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "seed": "npm run migrate && sequelize db:seed:all",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate"
  },
  "author": "Julie Wanja",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jm-wanja/document_management_system.git"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "colors": "^1.1.2",
    "dotenv-webpack": "^1.5.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "material-ui": "^0.18.3",
    "morgan": "^1.8.2",
    "open": "0.0.5",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.4",
    "webpack": "2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "rest-bac": "^1.0.0",
